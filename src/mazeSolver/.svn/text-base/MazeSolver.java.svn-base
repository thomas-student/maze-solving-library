package mazeSolver;

import maze.Maze;

/**
 * The main interface for MazeSolver, simply has one method, solveMazewhich solves the maze that 
 * this instance is instantiated with.
 * 
 *  @author Thomas Johnston
 */
public interface MazeSolver {

	/**
	 * @return An array of coordinates resembling the path, or null if not solvable
	 */
	public int[][] solveMaze(Maze maze);
	
}
